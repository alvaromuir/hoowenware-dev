<h3>Latest Trips</h3>
<%= link_to "Create a Trip", new_trip_path %>
<div>
  <% @trips.each do |trip| %>
    <% if trip.is_active? %>
    <div>
      <%= trip.start_date.to_date.strftime("%m/%d/%y")  %> 
      - 
      <%= trip.end_date.to_date.strftime("%m/%d/%y")   %>
    </div>
    </div>
      <%= link_to trip.title, trip %>
    </div>
    <div>
    <%= trip.location %>
    </div>
    <% end %>
  <% end %>
</div>