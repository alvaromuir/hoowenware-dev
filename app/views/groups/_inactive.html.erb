  <% @groups.each do |group| %>
    <% if group.is_active == false %>
      <div>
        <%= group.start_date.to_date.strftime("%m/%d/%y")  %> 
        - 
        <%= group.end_date.to_date.strftime("%m/%d/%y")   %>
      </div>
      <div>
        <%= link_to group.name, group %>
      </div>
      <div>
      <%= group.location %>
      </div>
    <% end %>
    <br />
  <% end %>